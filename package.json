    "test": "pnpm -r test",
    "format": "pnpm -r format",
    "format:web": "pnpm --filter web format",
    "format:mobile": "pnpm --filter mobile format",
    "format:common": "pnpm --filter common format",
    "lint:api": "pnpm --filter api lint",
    "lint:web": "pnpm --filter web lint",
    "lint:mobile": "pnpm --filter mobile lint",
    "lint:common": "pnpm --filter common lint",
    "type:web": "pnpm --filter web type",
    "type:mobile": "pnpm --filter mobile type",
    "type:common": "pnpm --filter common type",
    "test:api": "pnpm --filter api test",
    "test:web": "pnpm --filter web test",
    "test:mobile": "pnpm --filter mobile test",
    "test:common": "pnpm --filter common test",
    "api": "pnpm --filter api dev",
    "mobile": "pnpm --filter mobile dev",
    "web": "pnpm --filter web dev",
    "dev": "pnpm -r dev",
    "ci": "\"pnpm format\" \"pnpm lint\" \"pnpm type\" \"pnpm test\"",
    "common": "pnpm --filter common common"
